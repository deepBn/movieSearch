<% include partials/header %>
    <div class="container">
        <h1><i class="fas fa-search"></i> Results</h1>
            <div class="row" id="movies">
                <% data["Search"].forEach( function (movies) { %>
                    <div class="col-md-3 col-sm-6">
                        <a href="/results/details/<%= movies["imdbID"]%>">
                            <div class="thumbnail">
                            <img id="thumb-img" src=<%= movies["Poster"] %>>
                                <div class="caption">
                                    <p><%= movies["Title"] %></p>
                                    <p><%= movies["Year"] %></p>
                                </div>
                            </div>
                        </a>
                    </div>
                <% }); %>
            </div>
    </div>
<% include partials/footer %>